{
  "name": "prompt-mixer-open-ai-llamaindex-connector",
  "version": "1.0.4",
  "description": "OpenAI LlamaIndex Connector leverage the power of advanced AI models provided by OpenAI, such as GPT",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint '**/*.{js,ts}' --quiet",
    "lint:fix": "eslint '**/*.{js,ts}' --fix",
    "format": "prettier '**/*.{js,ts}' --write",
    "format:check": "prettier '**/*.{js,ts}' --check"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.21.3",
    "openai": "^4.33.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  }
}
